fun first(toys: NonEmptyList<Toy>): Toy {
  return toys[0]
}

fun silly(a: Int): Boolean {
  return if (a % 2 == 0) {
    true
  } else {
    false
  }
}

fun roadNotTaken(): String {
  return """
    Two roads diverged in a yellow wood,
    And sorry I could not travel both
    And be one traveler, long I stood
    And looked down one as far as I could
    To where it bent in the undergrowth;
  """.trimIndent()
}

fun firstThreeAlphabets(): List<Char> {
  return listOf('a', 'b', 'c')
}
